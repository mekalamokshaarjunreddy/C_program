#include <stdio.h>

int main() {
    int a,b,c,d,e,f,g,h,sum;
    float p;

    printf("Enter subject marks: ");
    scanf("%d %d %d %d %d %d %d %d", &a, &b, &c, &d, &e, &f, &g, &h);

    sum = a + b + c + d + e + f + g + h;
    p = (sum / 800.0) * 100;

    if(p >= 80 && p <= 100)
        printf("Excellent");
    else if(p >= 70 && p < 80)
        printf("Very Good");
    else if(p >= 60 && p < 70)
        printf("Good");
    else if(p >= 50 && p < 60)
        printf("Satisfy");
    else
        printf("Bad");

    return 0;
}
